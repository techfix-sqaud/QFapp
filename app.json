{
  "expo": {
    "name": "instant",
    "slug": "instant",
    "scheme": "your-app-scheme",
    "version": "1.0.0",
    "orientation": "portrait",
    "jsEngine": "hermes",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "techfix.quickFix"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png"
      },
      "package": "techfix.quickFix"
    },
    "web": {
      "bundler": "webpack",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "a79f1c9a-af65-4d9c-99e7-649d6fdfef1c"
      }
    },
    "owner": "techfix"
  }
}
